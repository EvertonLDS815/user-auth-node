{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js",
            "handle": "vercel",
            "method": "GET,HEAD,PUT,PATCH,POST,DELETE",
            "origin": "http://localhost:5173",
            "credentials": true
          }
    ]
}